<!-- These page divs were taken from an empty book created from the Basic Book template, Traditional XMatter, and default branding in Bloom 4.6 on January 30, 2020 -->
<Pages>
  <div class="bloom-page cover coverColor bloom-frontMatter frontCover outsideFrontCover A5Portrait side-right" data-page="required singleton" data-export="front-matter-cover" data-xmatter-page="frontCover" id="Front Cover" data-page-number="" lang="">
    <div data-after-content="Traditional Front/Back Matter" class="pageLabel" data-i18n="TemplateBooks.PageLabel.Front Cover" lang="en">Front Cover</div>
    <div class="pageDescription" lang="en"></div>
    <div class="marginBox">
      <div class="bloom-translationGroup bookTitle" data-default-languages="V,N1">
        <label class="bubble">Book title in {lang}</label>
        <div style="" class="bloom-editable bloom-nodefaultstylerule Title-On-Cover-style bloom-padForOverflow" data-book="bookTitle" lang="z" contenteditable="true"><p></p></div>
      </div>
      <div class="bloom-imageContainer">
        <img data-book="coverImage" src="placeHolder.png" alt=""></img>
        <div class="bloom-translationGroup bloom-imageDescription bloom-trailingElement" data-default-languages="auto">
          <div style="" class="bloom-editable ImageDescriptionEdit-style" data-book="coverImageDescription" lang="z" contenteditable="true"></div>
        </div>
      </div>
      <div class="bottomBlock">
        <div data-book="cover-branding-left-html" lang="*"></div>
        <div class="bottomTextContent">
          <div aria-describedby="qtip-0" data-hasqtip="true" class="creditsRow" data-hint="You may use this space for author/illustrator, or anything else.">
            <div class="bloom-translationGroup" data-default-languages="V">
              <div style="" class="bloom-editable smallCoverCredits Cover-Default-style" data-book="smallCoverCredits" lang="z" contenteditable="true"></div>
            </div>
          </div>
          <div class="bottomRow" data-have-topic="false">
            <div class="coverBottomLangName Cover-Default-style" data-derived="languagesOfBook" lang="en"></div>
            <div aria-describedby="qtip-2" data-hasqtip="true" class="coverBottomBookTopic bloom-userCannotModifyStyles bloom-alwaysShowBubble Cover-Default-style" data-derived="topic" data-functiononhintclick="ShowTopicChooser()" data-hint="Click to choose topic"></div>
          </div>
        </div>
      </div>
    </div>
  </div>

  <div class="bloom-page numberedPage customPage bloom-combinedPage A5Portrait bloom-monolingual side-right" data-page="" id="Basic Text &amp; Picture" data-pagelineage="adcd48df-e9ab-4a07-afd4-6a24d0398382" data-page-number="1" lang="">
    <div class="pageLabel" data-i18n="TemplateBooks.PageLabel.Basic Text &amp; Picture" lang="en">Basic Text &amp; Picture</div>
    <div class="pageDescription" lang="en"></div>
    <div class="marginBox">
      <div style="min-height: 42px;" class="split-pane horizontal-percent">
        <div class="split-pane-component position-top" style="bottom: 50%">
          <div class="split-pane-component-inner">
            <div class="bloom-imageContainer bloom-leadingElement"><img src="placeHolder.png" alt=""></img></div>
          </div>
        </div>
        <div class="split-pane-divider horizontal-divider" style="bottom: 50%"></div>
        <div class="split-pane-component position-bottom" style="height: 50%">
          <div class="split-pane-component-inner">
            <div class="bloom-translationGroup bloom-trailingElement" data-default-languages="auto">
              <div style="" class="bloom-editable normal-style" lang="z" contenteditable="true"><p></p></div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>

  <div class="bloom-page numberedPage customPage bloom-combinedPage A5Portrait bloom-monolingual side-left" data-page="" id="Picture in Middle" data-pagelineage="adcd48df-e9ab-4a07-afd4-6a24d0398383" data-page-number="2" lang="">
    <div class="pageLabel" data-i18n="TemplateBooks.PageLabel.Picture in Middle" lang="en">Picture in Middle</div>
    <div class="pageDescription" lang="en"></div>
    <div class="marginBox">
      <div style="min-height: 42px;" class="split-pane horizontal-percent">
        <div class="split-pane-component position-top" style="bottom: 76%">
          <div class="split-pane-component-inner">
            <div class="bloom-translationGroup bloom-trailingElement" data-default-languages="auto">
              <div style="" class="bloom-editable normal-style" lang="z" contenteditable="true"><p></p></div>
            </div>
          </div>
        </div>
        <div class="split-pane-divider horizontal-divider" style="bottom: 76%"></div>
        <div class="split-pane-component position-bottom" style="height: 76%">
          <div class="split-pane-component-inner">
            <div style="min-height: 48px;" class="split-pane horizontal-percent">
              <div class="split-pane-component position-top" style="bottom: 30%">
                <div class="split-pane-component-inner">
                  <div class="bloom-imageContainer bloom-leadingElement"><img src="placeHolder.png" alt=""></img></div>
                </div>
              </div>
              <div class="split-pane-divider horizontal-divider" style="bottom: 30%"></div>
              <div class="split-pane-component position-bottom" style="height: 30%">
                <div class="split-pane-component-inner">
                  <div class="bloom-translationGroup bloom-trailingElement" data-default-languages="auto">
                    <div style="" class="bloom-editable normal-style" lang="z" contenteditable="true"><p></p></div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>

  <div class="bloom-page numberedPage customPage bloom-combinedPage A5Portrait bloom-monolingual side-right" data-page="" id="Picture on Bottom" data-pagelineage="adcd48df-e9ab-4a07-afd4-6a24d0398384" data-page-number="3" lang="">
    <div class="pageLabel" data-i18n="TemplateBooks.PageLabel.Picture on Bottom" lang="en">Picture on Bottom</div>
    <div class="pageDescription" lang="en"></div>
    <div class="marginBox">
      <div style="min-height: 42px;" class="split-pane horizontal-percent">
        <div class="split-pane-component position-top" style="bottom: 50%">
          <div class="split-pane-component-inner">
            <div class="bloom-translationGroup bloom-trailingElement" data-default-languages="auto">
              <div style="" class="bloom-editable normal-style" lang="z" contenteditable="true"><p></p></div>
            </div>
          </div>
        </div>
        <div class="split-pane-divider horizontal-divider" style="bottom: 50%"></div>
        <div class="split-pane-component position-bottom" style="height: 50%">
          <div class="split-pane-component-inner">
            <div class="bloom-imageContainer"><img src="placeHolder.png" alt=""></img></div>
          </div>
        </div>
      </div>
    </div>
  </div>

  <div class="bloom-page numberedPage customPage A5Portrait bloom-monolingual side-left" data-page="" id="Just a Picture" data-pagelineage="adcd48df-e9ab-4a07-afd4-6a24d0398385" data-page-number="4" lang="">
    <div class="pageLabel" data-i18n="TemplateBooks.PageLabel.Just a Picture" lang="en">Just a Picture</div>
    <div class="pageDescription" lang="en"></div>
    <div class="marginBox">
      <div class="split-pane-component-inner">
        <div class="bloom-imageContainer"><img src="placeHolder.png" alt=""></img></div>
      </div>
    </div>
  </div>

  <div class="bloom-page numberedPage customPage A5Portrait side-right bloom-monolingual" data-page="" id="Just Text" data-pagelineage="a31c38d8-c1cb-4eb9-951b-d2840f6a8bdb" data-page-number="5" lang="">
    <div class="pageLabel" data-i18n="TemplateBooks.PageLabel.Just Text" lang="en">Just Text</div>
    <div class="pageDescription" lang="en"></div>
    <div class="marginBox">
      <div class="split-pane-component-inner">
        <div class="bloom-translationGroup bloom-trailingElement" data-default-languages="auto">
          <div style="" class="bloom-editable normal-style" lang="z" contenteditable="true"><p></p></div>
        </div>
      </div>
    </div>
  </div>
</Pages>
